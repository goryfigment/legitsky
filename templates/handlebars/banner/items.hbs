{{#each items}}
    <div class="item" data-category="{{category}}">
        <img src="{{concat '/templates/bundle/assets/banner/' 'banner_name' '/' picture}}"/>
        <div class="item-details-wrapper">
            <a class="item-name" href="{{url}}" title="{{name}}" target="_blank">{{name}}</a>
            <div class="item-price">{{price}}</div>
        </div>
    </div>
{{/each}}